{
  "root": true,
  "rules": {
    "arrow-parens": [
      2,
      "as-needed"
    ],
    "arrow-spacing": 2,
    "brace-style": [
      2,
      "1tbs",
      {
        "allowSingleLine": false
      }
    ],
    "camelcase": [
      2,
      {
        "properties": "always"
      }
    ],
    "comma-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "comma-style": [
      2,
      "last"
    ],
    "computed-property-spacing": [
      2,
      "never"
    ],
    "constructor-super": 2,
    "curly": 2,
    "eol-last": 2,
    "eqeqeq": [
      2,
      "allow-null"
    ],
    "func-style": [
      2,
      "declaration",
      {
        "allowArrowFunctions": true
      }
    ],
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "jsx-quotes": [
      2,
      "prefer-double"
    ],
    "key-spacing": [
      2,
      {
        "beforeColon": false,
        "afterColon": true
      }
    ],
    "new-cap": 2,
    "new-parens": 2,
    "no-alert": 1,
    "no-array-constructor": 2,
    "no-constant-condition": 2,
    "no-class-assign": 2,
    "no-console": 1,
    "no-const-assign": 2,
    "no-debugger": 1,
    "no-dupe-class-members": 2,
    "no-eval": 2,
    "no-fallthrough": 0,
    "no-floating-decimal": 2,
    "no-implicit-coercion": 2,
    "no-implied-eval": 2,
    "no-lonely-if": 2,
    "no-loop-func": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multi-spaces": 2,
    "no-native-reassign": 2,
    "no-negated-condition": 2,
    "no-nested-ternary": 2,
    "no-new": 2,
    "no-new-func": 2,
    "no-new-object": 2,
    "no-new-wrappers": 2,
    "no-self-compare": 2,
    "no-shadow": 1,
    "no-spaced-func": 2,
    "no-this-before-super": 2,
    "no-trailing-spaces": 2,
    "no-undefined": 2,
    "no-underscore-dangle": 2,
    "no-var": 2,
    "no-void": 2,
    "no-with": 2,
    "object-curly-spacing": [
      2,
      "always"
    ],
    "object-shorthand": 2,
    "prefer-const": 2,
    "prefer-spread": 2,
    "quote-props": [
      2,
      "as-needed"
    ],
    "quotes": [
      2,
      "single",
      "avoid-escape"
    ],
    "radix": 2,
    "react/react-in-jsx-scope": 0,
    "semi": [
      2,
      "always"
    ],
    "sort-vars": 1,
    "space-before-blocks": 2,
    "spaced-comment": [
      2,
      "always"
    ],
    "valid-jsdoc": [
      2,
      {
        "requireParamDescription": false,
        "requireReturnDescription": false
      }
    ],
    "yoda": 2
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "arrowFunctions": true,
      "blockBindings": true,
      "classes": true,
      "destructuring": true,
      "modules": true,
      "objectLiteralComputedProperties": true,
      "objectLiteralShorthandMethods": true,
      "objectLiteralShorthandProperties": true,
      "restParams": true,
      "templateStrings": true
    },
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "globals": {
    "require": false,
    "process": false,
    "describe": false,
    "it": false,
    "expect": false,
    "sinon": false,
    "before": false,
    "beforeEach": false,
    "afterEach": false
  }
}
